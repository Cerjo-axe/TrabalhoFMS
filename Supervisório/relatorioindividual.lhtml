</select></center>


<script type="text/javascript">
var select = document.getElementById("data"); 
var data = new Date();
var e;
for(var i = 0; i < 30; i++) {
    var mes="";
    var dia="";
    mes = (data.getMonth()+1).toString();
    if (mes.length==1) {
     mes = "0"+mes;
    }
    var dia="";
     dia = data.getDate().toString();
    if (dia.length==1) {
     dia = "0"+dia;
    }
    var opt = dia+"/"+mes+"/"+data.getFullYear().toString();
    var el = document.createElement("option");
    el.textContent = opt;
    el.value = opt;
    select.appendChild(el);
    e = data.getTime() - 1000*60*60*24*1;
    data.setTime(e);
}
</script>

